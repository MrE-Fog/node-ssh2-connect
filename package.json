{
  "name": "ssh2-connect",
  "description": "Callback-based api behind ssh2 to open an SSH connection",
  "version": "2.0.5",
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    },
    "no_backticks": {
      "level": "ignore"
    },
    "no_nested_string_interpolation": {
      "level": "ignore"
    }
  },
  "contributors": [],
  "dependencies": {
    "ssh2": "~0.8.2"
  },
  "devDependencies": {
    "coffeescript": "^2.3.2",
    "mocha": "~5.2.0",
    "should": "~13.2.3"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "homepage": "https://github.com/adaltas/node-ssh2-connect",
  "keywords": [
    "client",
    "exec",
    "remote",
    "secure",
    "sftp",
    "shell",
    "ssh",
    "ssh2"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "optionalDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/adaltas/node-ssh2-connect.git"
  },
  "scripts": {
    "preversion": "grep '## Trunk' CHANGELOG.md && npm test",
    "version": "version=`grep '^  \"version\": ' package.json | sed 's/.*\"\\([0-9\\.]*\\)\".*/\\1/'` && sed -i \"s/## Trunk/## Version $version/\" CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "patch": "npm version patch -m 'Bump to version %s'",
    "minor": "npm version minor -m 'Bump to version %s'",
    "major": "npm version major -m 'Bump to version %s'",
    "build": "coffee -b -o lib src",
    "pretest": "npm run build",
    "test": "mocha 'test/**/*.coffee'"
  }
}
